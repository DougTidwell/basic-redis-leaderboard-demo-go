{
  "name": "Basic Redis Leaderboard Demo Golang",
  "description": "List of top 100 companies",
  "stack": "container",
  "repository": "https://github.com/redis-developer/basic-redis-leaderboard-demo-go",
  "logo": "https://redis.io/images/redis-white.png",
  "keywords": ["golang", "gin", "redis", "leadboard"],
  "env": {
    "REDIS_HOST": {
      "description": "Redis server host",
      "required": true
    },
    "REDIS_PORT": {
      "description": "Redis server PORT",
      "required": true
    },
    "REDIS_PASSWORD": {
	    "description": "Redis server password",
	    "required": true
    },
    "API_PUBLIC_PATH": {
      "description": "Public path to frontend",
      "value": "/api/public"
    },
    "IMPORT_PATH": {
      "description": "Path to seed.json file for import",
      "value": "/api/seed.json"
    },
    "API_PORT": {
      "description": "Api public port",
      "value": 5000
    },
    "API_TLS_DISABLED": {
      "description": "Disable TLS",
      "value": true
    }
  }
}